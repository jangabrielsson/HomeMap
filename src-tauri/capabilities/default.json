{
  "identifier": "default",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-create",
    "core:webview:allow-internal-toggle-devtools",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "http://*/*"
        },
        {
          "url": "https://*/*"
        }
      ]
    },
    "fs:default",
    {
      "identifier": "fs:allow-read-file",
      "allow": [
        {
          "path": "$HOME/**"
        },
        {
          "path": "$RESOURCE/**"
        }
      ]
    },
    {
      "identifier": "fs:allow-read-dir",
      "allow": [
        {
          "path": "$HOME/**"
        },
        {
          "path": "$RESOURCE/**"
        }
      ]
    },
    "fs:allow-exists",
    "dialog:default",
    "process:default",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download",
    "updater:allow-download-and-install",
    "updater:allow-install"
  ]
}
