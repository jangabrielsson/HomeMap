{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "HomeMap",
  "version": "0.1.7",
  "identifier": "com.gabrielsson.homemap",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main",
        "title": "HomeMap",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../homemapdata.example"
    ],
    "macOS": {
      "signingIdentity": "-"
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/jangabrielsson/HomeMap/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDQ4OTU4QUJDNEI1QjdBQzgKUldUSWVsdEx2SXFWU0s0NU9YZnJVL0w3YVFVZXZrc01YWER2VGdvUmdyWmdNNlB6NGdwWXlPUDcK"
    }
  }
}
